{
  "name": "express-react-ts-template",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-vue": "^2.2.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^17.0.15",
    "@types/passport": "^1.0.7",
    "@types/passport-twitch-latest": "^1.0.1",
    "chokidar": "^3.5.3",
    "concurrently": "^7.0.0",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1",
    "prisma": "^3.9.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@prisma/client": "3.9.1",
    "axios": "^0.25.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "passport": "^0.5.2",
    "passport-session": "^1.0.2",
    "passport-twitch-latest": "^1.0.0",
    "vue": "3",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "scripts": {
    "build": "yarn server:build && yarn client:build",
    "dev": "concurrently \"yarn server:dev\" \"yarn client:dev\"",
    "server:dev": "ts-node src",
    "client:dev": "parcel watch client/app.html --dist-dir dist/client",
    "client:build": "parcel build client/app.html --dist-dir dist/client",
    "server:build": "tsc -p tsconfig.server.json"
  }
}
